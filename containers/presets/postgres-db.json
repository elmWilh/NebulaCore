{
  "name": "postgres-db",
  "title": "PostgreSQL",
  "description": "PostgreSQL database preset",
  "config": {
    "image": "postgres:16",
    "ram": 2048,
    "swap": 4096,
    "disk": 40,
    "cpu": 1024,
    "cpu_limit": 1.5,
    "cpuset": "",
    "cpu_quota": "",
    "cpu_period": "",
    "pids_limit": 256,
    "shm": 256,
    "ports": "5432:5432",
    "command": "",
    "env": "POSTGRES_PASSWORD=change_me\nPOSTGRES_DB=appdb",
    "volumes": "/srv/postgres:/var/lib/postgresql/data:rw",
    "workspace_mount": "/var/lib/postgresql/data",
    "explorer_root": "/var/lib/postgresql/data",
    "console_cwd": "/var/lib/postgresql/data",
    "profile_name": "postgres-db",
    "restart": true
  },
  "permissions": {
    "user": { "allow_explorer": false, "allow_root_explorer": false, "allow_console": false, "allow_shell": false, "allow_settings": false, "allow_edit_files": false, "allow_edit_startup": false, "allow_edit_ports": false },
    "developer": { "allow_explorer": true, "allow_root_explorer": false, "allow_console": true, "allow_shell": true, "allow_settings": true, "allow_edit_files": true, "allow_edit_startup": true, "allow_edit_ports": true }
  }
}
